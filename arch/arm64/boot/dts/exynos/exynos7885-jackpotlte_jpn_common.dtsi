		};
		ssp_rst: ssp-rst {
			samsung,pins = "gpg3-5";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <3>;
			samsung,pin-pud-pdn = <0>;
		};

		spi4_clk: spi4-clk {
			samsung,pins = "gpp8-1";
			samsung,pin-function = <2>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <2>;
		};
		spi4_cs: spi4-cs {
			samsung,pins = "gpp8-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <2>;
		};
		spi4_miso: spi4-miso {
			samsung,pins = "gpp7-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
